@{
    List<TipoPrueba> tiposPrueba = (List<TipoPrueba>) ViewData["tiposPrueba"];
}
<div class="card-body table-full-width table-responsive">
    <table class="table table-hover">
        <thead>
            <th>Nombre</th>
            <th>Parametros</th>
            <th>Descripción</th>
            <th>Cantidad Scripts</th>
            <th>Opciones</th>
        </thead>
        <tbody>
            @foreach(TipoPrueba tp in tiposPrueba)
            {
            <tr>
                <td>@(tp.Nombre)</td>
                <td>@(tp.Parametros)</td>
                <td>@(tp.Descripcion)</td>
                <td></td>
                <td>
                    <span style="cursor:pointer;" data-toggle="tooltip" data-placement="top" title="Editar"><i class="nc-icon nc-settings-gear-64"></i></span>
                    &nbsp;&nbsp;
                    <span style="cursor:pointer;" data-toggle="modal" data-target="#myModal" data-placement="top" title="Ver Script"><i class="nc-icon nc-paper-2"></i></span>
                    &nbsp;&nbsp;
                    <span style="cursor:pointer;" data-toggle="tooltip" data-placement="top" title="Eliminar"><i class="nc-icon nc-simple-remove"></i></span>

                </td>
            </tr>
            }
        </tbody>
    </table>
</div>